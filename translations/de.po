#: objects.py:1258
msgid "Title.stub_from_search(): row is None."
msgstr "Title.stub_from_search(): row is None."

#: objects.py:1301
msgid "Title.stub_from_simple_search(): row is None."
msgstr "Title.stub_from_search(): hat keine Daten geliefert."

